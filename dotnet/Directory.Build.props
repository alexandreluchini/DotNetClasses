<Project>
	<PropertyGroup>
		<AssemblyVersion>11.0.0.0</AssemblyVersion>
		<MajorFileVersion>1</MajorFileVersion>
		<MinorFileVersion>4</MinorFileVersion>
		<FileVersion>$(MajorFileVersion).$(MinorFileVersion).0</FileVersion>
		<InformationalVersion>$([System.DateTime]::UtcNow.ToString("yyyyMMddHHmmss")).$(GITHUB_SHA)</InformationalVersion>
		<AssemblyCompany>GeneXus</AssemblyCompany>
		<AssemblyCulture></AssemblyCulture>
		<AssemblyProduct>GeneXus</AssemblyProduct>
		<AssemblyTrademark></AssemblyTrademark>
		<AssemblyCopyright>Copyright Â© 1988-2019 GeneXus. All Rights Reserved</AssemblyCopyright>
		<AssemblyOriginatorKeyFile>$(MSBuildThisFileDirectory)GXclasses.snk</AssemblyOriginatorKeyFile>
		<GenerateAssemblyInfo>true</GenerateAssemblyInfo>
		<SignAssembly>true</SignAssembly>
		<TreatWarningsAsErrors>true</TreatWarningsAsErrors>
		<WarningLevel>4</WarningLevel>
		<RunCodeAnalysis>false</RunCodeAnalysis>
		<CodeAnalysisRuleSet>$(MSBuildThisFileDirectory)StandardClasses.ruleset</CodeAnalysisRuleSet>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<RepositoryType>git</RepositoryType>
		<RepositoryUrl>https://github.com/genexuslabs/dotnetClasses</RepositoryUrl>
		<NoWarn>NU5105;CS0618</NoWarn>
	</PropertyGroup>

	
	<PropertyGroup Condition="!$(GITHUB_REF.EndsWith('master'))">
		<MajorFileVersion>$([MSBuild]::Add($(MajorFileVersion), 100))</MajorFileVersion>
	</PropertyGroup>


	<Target Name="GetFileVersion">
		<Message Importance="high" Text="FileVersion:$(FileVersion)"></Message>
	</Target>
	<Target Name="IsMaster">
		<Message Importance="high" Text="IsMaster: True" Condition="$(GITHUB_REF.EndsWith('master'))"></Message>
	</Target>
</Project>